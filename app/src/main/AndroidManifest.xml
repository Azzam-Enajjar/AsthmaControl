<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.pdx.oss.asthmacontrol" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewLogActivity"
            android:label="@string/title_activity_new_log" >
        </activity>
        <activity
            android:name=".UpdateLogMenu"
            android:label="@string/title_activity_update_log_menu" >
        </activity>
        <activity
            android:name=".UpdateAsthmaTimeActivity"
            android:label="@string/title_activity_update_asthma_time" >
        </activity>
    </application>

</manifest>
